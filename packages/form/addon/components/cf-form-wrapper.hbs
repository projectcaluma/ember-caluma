{{#let
  (component
    (ensure-safe-component
      (get-widget @fieldset.field.question @fieldset.form default="cf-form")
    )
  )
  as |FormComponent|
}}
  <FormComponent
    @document={{@document}}
    @fieldset={{@fieldset}}
    @context={{@context}}
    @disabled={{@disabled}}
    @onSave={{@onSave}}
  />
{{/let}}