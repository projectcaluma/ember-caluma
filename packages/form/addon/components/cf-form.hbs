<form novalidate>
  {{#each @fieldset.fields as |field|}}
    {{#if (has-block)}}
      {{yield field}}
    {{else}}
      <CfField
        @field={{field}}
        @disabled={{@disabled}}
        @context={{@context}}
        @compare={{@compare}}
        @onSave={{@onSave}}
      />
    {{/if}}
  {{/each}}
</form>