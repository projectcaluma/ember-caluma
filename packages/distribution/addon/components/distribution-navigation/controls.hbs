<div class="uk-text-center uk-margin-small-top">
  {{#if this.canCreate}}
    <IconButton @route="distribution.new" @icon="plus" />
  {{/if}}
  <IconButton
    @icon="paper-plane-outline"
    @fromSvgJar={{true}}
    @gutterTop={{4}}
    @gutterRight={{6}}
    @gutterLeft={{4}}
    @gutterBottom={{4}}
    @onClick={{this.noop}}
  />
  {{#if this.canComplete}}
    <IconButton
      @icon="lock-closed-outline"
      @fromSvgJar={{true}}
      @onClick={{this.noop}}
    />
  {{/if}}
</div>